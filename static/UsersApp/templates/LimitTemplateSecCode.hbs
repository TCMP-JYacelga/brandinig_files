<div id="limSecCodeRow">
<div class="row">
	<div class="col-sm-6">
		{{#each assets}}
			{{#selectAsset assetId "18"}}
				<div class="panel panel-default">
					<div class="panel-heading">
						<div class="row">
							<div class="col-sm-12">
								<h2 class="panel-title">
									<span id="secCodeMakerInfoCaret" class="fa fa-caret-up pull-left cursor_pointer toggle-handler"></span>
									<span>{{getLabel "lbl.clientUser.limit.secCodeMakerInfoCaret" "Maker - SEC Code"}}</span>
									<img id = "makerLimit_SecCode" data-makerSecCodeEnabled="{{ makerSecCodeEnabled }}" tabindex="1" 
										 src="{{checkBoxIcon makerSecCodeEnabled}}" class="chkBoxAlign" width="16" height="16" border="0">
								</h2>
							</div>
						</div>
					</div>
					<div id="secCodeMakerInfoDiv"  class="panel-body">
						<div class="row">
							<div class="col-sm-6 form-group">
								<label>{{getLabel "lbl.clientUser.limit.templateSecCode.defaultLimitProfile" "Default Limit Profile"}}</label>
								
								{{#if makerSecCodeEnabled}}
									<select id="defMaker_SecCode" name="defMaker_SecCode" class="form-control" tabindex="1">
								{{else}}
									<select id="defMaker_SecCode" name="defMaker_SecCode" class="form-control" tabindex="1" disabled="disabled">
								{{/if}}	
									<option value="Select" >{{getLabel "lbl.clientUser.select" "Select"}}</option>
									{{#each profileList}}
										{{option profileId profileDescription assignedFlag }}
									{{/each}}
								</select>
							</div>
						</div>
						
						
								<div class="grid">
									<table class="view-grid" style="width:100%;table-layout: fixed;">
										<thead>
											<tr class="view-grid-header">
												<td class="view-grid-header-text" style="width: 60%;">{{getLabel "lbl.clientUser.limit.secCodes" "SEC Codes"}}</td>
												<td class="view-grid-header-text" style="width: 40%;">{{getLabel "lbl.clientUser.limit.secCodes.limitPrf" "Limit Profile"}}</td>
											</tr>
														
										</thead>			
										<tbody>	
										{{#if makerSecCodeEnabled}}
											  {{#each makerSecCode}}
											  		<tr class="view-grid-body">
											  			<td class="view-grid-body-content textWrap" title="{{categoryDescription}}" id="{{categoryCode}}" style="width: 60%;">{{categoryDescription}}</td>
											  			<td class="view-grid-body-content" style="width: 40%;">
										  					<select id="SecCode_maker_{{categoryCode}}" name="SecCode_maker_{{categoryCode}}" 
											  					data-categoryCode="{{ categoryCode }}" data-categoryCode="{{ categoryDescription }}" class="form-control profileSelect" tabindex="1">
																	<option value="Select" >{{getLabel "lbl.clientUser.select" "Select"}}</option>
																	{{custProfileSelectOption ../profileList tempTypeCode tempTypeDescription }}																	
															</select>
											  			</td>
													</tr>
											  {{else}}
													<tr class="view-grid-body">
															<td class="view-grid-body-content" colSpan="2" style="padding-bottom: 1%;" >&nbsp</td>
													</tr>
											 {{/each}}	
										{{else}}
											 {{#each makerSecCode}}
										  		<tr class="view-grid-body">
										  			<td class="view-grid-body-content textWrap" title="{{categoryDescription}}">{{categoryDescription}}</td>
										  			<td class="view-grid-body-content">
											  			<select id="SecCode_maker_{{categoryCode}}" name="SecCode_maker_{{categoryCode}}" data-categoryCode="{{ categoryCode }}" data-categoryCode="{{ categoryDescription }}" class="form-control profileSelect" disabled="disabled" tabindex="1">
															<option value="Select" >{{getLabel "lbl.clientUser.select" "Select"}}</option>
															{{custProfileSelectOption ../profileList tempTypeCode tempTypeDescription }}																	
														</select>
										  			</td>
												</tr>
											 {{/each}}	
										{{/if}}	 
										</tbody>
									</table>
								</div>
							
					</div>
				</div>
			{{/selectAsset}}
		{{/each}}
	</div>
	<div class="col-sm-6">
		{{#each assets}}
			{{#selectAsset assetId "18"}}
				<div class="panel panel-default">
					<div class="panel-heading">
						<div class="row">
							<div class="col-sm-12">
								<h2 class="panel-title">
									<span id="secCodeCheckerInfoCaret" class="fa fa-caret-up pull-left cursor_pointer toggle-handler"></span>
									<span>{{getLabel "lbl.clientUser.limit.secCodeCheckerInfoCaret" "Checker - SEC Code"}}</span>
									<img id = "checkerLimit_SecCode" data-checkerSecCodeEnabled = "{{ checkerSecCodeEnabled }}" tabindex="1"
										 src="{{checkBoxIcon checkerSecCodeEnabled}}" class="chkBoxAlign" width="16" height="16" border="0">
								</h2>
							</div>
						</div>
					</div>
					<div id="secCodeCheckerInfoDiv"  class="panel-body">
						<div class="row">
							<div class="col-sm-6 form-group">
								<label>{{getLabel "lbl.clientUser.limit.templateSecCode.defaultLimitProfile" "Default Limit Profile"}}</label>
								
								{{#if checkerSecCodeEnabled}}
									<select id="defChecker_SecCode" name="defChecker_SecCode" class="form-control" tabindex="1">
								{{else}}
									<select id="defChecker_SecCode" name="defChecker_SecCode" class="form-control" tabindex="1" disabled="disabled">
								{{/if}}	
									<option value="Select" >{{getLabel "lbl.clientUser.select" "Select"}}</option>
									{{#each profileList}}
										{{option profileId profileDescription assignedFlag }}
									{{/each}}
								</select>
							</div>
						</div>
						
					
								<div class="grid">
									<table class="view-grid" style="width:100%;table-layout: fixed;">
										<thead>
											<tr class="view-grid-header">
												<td class="view-grid-header-text" style="width: 60%;">{{getLabel "lbl.clientUser.limit.secCodes" "SEC Codes"}}</td>
												<td class="view-grid-header-text" style="width: 40%;">{{getLabel "lbl.clientUser.limit.secCodes.limitPrf" "Limit Profile"}}</td>
											</tr>
														
										</thead>			
										<tbody>	
										{{#if checkerSecCodeEnabled}}
											  {{#each checkerSecCode}}
											  		<tr class="view-grid-body">
											  			<td class="view-grid-body-content textWrap" title="{{categoryDescription}}" id="{{categoryCode}}" style="width: 60%;">{{categoryDescription}}</td>
											  			<td class="view-grid-body-content" style="width: 40%;">
										  					<select id="SecCode_checker_{{categoryCode}}" name="SecCode_checker_{{categoryCode}}" 
											  					data-categoryCode="{{ categoryCode }}" data-categoryCode="{{ categoryDescription }}" class="form-control profileSelect" tabindex="1">
																	<option value="Select" >{{getLabel "lbl.clientUser.select" "Select"}}</option>
																	{{custProfileSelectOption ../profileList tempTypeCode tempTypeDescription }}																	
															</select>
											  			</td>
													</tr>
											  {{else}}
													<tr class="view-grid-body">
															<td class="view-grid-body-content" colSpan="2" style="padding-bottom: 1%;" >&nbsp</td>
													</tr>
											 {{/each}}	
										{{else}}
											 {{#each checkerSecCode}}
										  		<tr class="view-grid-body">
										  			<td class="view-grid-body-content textWrap" title="{{categoryDescription}}">{{categoryDescription}}</td>
										  			<td class="view-grid-body-content">
											  			<select id="SecCode_checker_{{categoryCode}}" name="SecCode_checker_{{categoryCode}}" data-categoryCode="{{ categoryCode }}" data-categoryCode="{{ categoryDescription }}" class="form-control profileSelect" disabled="disabled" tabindex="1">
															<option value="Select" >{{getLabel "lbl.clientUser.select" "Select"}}</option>
															{{custProfileSelectOption ../profileList tempTypeCode tempTypeDescription }}																	
														</select>
										  			</td>
												</tr>
											 {{/each}}	
										{{/if}}	 
										</tbody>
									</table>
								</div>
							
					</div>
				</div>
			{{/selectAsset}}
		{{/each}}
	</div>
</div>
</div>