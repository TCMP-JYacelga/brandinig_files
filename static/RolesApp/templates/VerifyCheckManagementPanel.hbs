<div>
				<div class="panel panel-default">						
					<div class="panel-heading">
						<div class="row">
							<div class="col-sm-12">
								<h2 class="panel-title">	
									<span id="checkMgmtCaret" class = "fa fa-caret-up pull-left cursor_pointer toggle-handler"></span>								
									<span class="ft-padding-l ">{{getLabel "lbl.rolesrv.14" "Check Management"}}</span>								
								</h2>
							</div>
						</div>
					</div>
					<div id="checkMgmtAssetBody" class="panel-body">
						<div id="checkMgmtFunctionalDiv">
						 {{#each assets}}
							{{#selectAsset assetId "14"}}
							 {{#if isFunction }}
							<table class="view-grid" style="width:100%">
								<tbody>
									<tr class="view-grid-header"><td class="view-grid-header-text" style="width:20%;">{{getLabel "lbl.role.privfunction" "Function"}}</td><td class="view-grid-header-text">{{getLabel "lbl.role.dtls" "Details"}}</td></tr>
									
											{{#if reports}}
												<tr class="view-grid-body">
													<td class="view-grid-body-content">{{getLabel "lbl.role.report" "Reports"}}</td>
													<td class="view-grid-body-content" style="width:80%;">
														{{getListExpression reports "reportType"}}	
													</td>
												</tr>
											{{/if}}
											{{#if alerts}}			
												<tr class="view-grid-body">
													<td class="view-grid-body-content">{{getLabel "lbl.role.alert" "Alerts"}}</td>
													<td class="view-grid-body-content" style="width:80%;">
														{{getListExpression alerts "alertType"}}	
													</td>
												</tr>
											{{/if}}	
										</tbody>
										{{/if}}	
									{{/selectAsset}}
								{{/each}}
							</table>
						</div>
					
					<!-- Start of check Management View Service - Privileges Table -->
						<div class="ft-margin-t" id="lmsPrivileges">
						{{#each assets}}
							{{#selectAsset assetId "14"}}
								{{#if permissions}}
								<table class="view-grid" style="width:100%">
									<thead>
										<tr><td colspan="25" class="view-grid-section-header">{{getLabel "lbl.role.privileges" "Privileges"}}</td></tr>
										<tr class="view-grid-header">
											<th class="view-grid-header-text" >{{getLabel "lbl.role.type" "Type"}}</th>
											<th class="view-grid-header-text" style="width:20%;">{{getLabel "lbl.role.privView" "View"}}</th>
											<th class="view-grid-header-text" style="width:20%;">{{getLabel "lbl.role.privEdit" "Edit"}}</th>
											<th class="view-grid-header-text" style="width:20%;">{{getLabel "lbl.role.privAppr" "Approve"}}</th>
										</tr>		
									</thead>
									<tbody>							
										{{#each permissions}}
											<tr class="view-grid-body">
												<td class="view-grid-header-text view-grid-body-content" >{{getColoredExpression colorClass screenName}}</td>
																{{#ifCond rmForView '==' "Y" }}
																{{#if view}}
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >
													<i class="fa fa-check"></i>
												</td>
																{{else}}
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" ></td>
																{{/if}}
																{{else}}
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" ></td>
																{{/ifCond}} 
																{{#ifCond rmForEdit '==' "Y" }}
																{{#if edit}}
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >
													<i class="fa fa-check"></i>
												</td>
																{{else}}
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" ></td>
																{{/if}}
																{{else}}
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" ></td>
																{{/ifCond}}
																{{#ifCond rmForAuth '==' "Y" }}	
																{{#if auth}}
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >
													<i class="fa fa-check"></i>
												</td>
																{{else}}
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" ></td>
																{{/if}}
																{{else}}
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" ></td>
																{{/ifCond}}
											</tr>
										{{else}}
											<td class="view-grid-header-text view-grid-body-content" colspan="4" style="width:20%;" >None Selected</td>											
										{{/each}}											
									</tbody>
								</table>
								{{/if}}	
								{{/selectAsset}}
							{{/each}}																			
						</div>
						<!-- End of check management View Service - Privileges Table -->	
							
						<!-- Start of check Management View - Account table -->							
						<div class="ft-margin-t" id="checkMgmtAccountTableDiv">
						{{#each assets}}
							{{#selectAsset assetId "14"}}
							  {{#unless granularPermission.checks}}
								{{#if accounts}}
								<table class="view-grid" style="width:100%">
									<thead>
										<tr><td colspan="25" class="view-grid-section-header">{{getLabel "lbl.role.acct" "Accounts"}}</td></tr>
										<tr class="view-grid-header">
											<th class="view-grid-header-text" style="width:20%;" >{{getLabel "lbl.role.acctNo" "Account No"}}</th>
											<th class="view-grid-header-text" style="width:20%;" >{{getLabel "lbl.role.acctNm" "Account Name"}}</th>
											<th class="view-grid-header-text" style="width:20%;" >{{getLabel "lbl.role.clientDesc" "Client Description"}}</th>
										</tr>		
									</thead>
									<tbody>							
										{{#each accounts}}
											<tr class="view-grid-body">
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >{{getColoredExpression colorClass accountNo}}</td>
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >{{getColoredExpression colorClass accountName}}</td>
												<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >{{getColoredExpression colorClass subsidiaryName}}</td>
											</tr>											
										{{/each}}											
									</tbody>
								</table>	
								{{/if}}
							  {{/unless}}	
							{{/selectAsset}}
						{{/each}}						
						</div>
						<!-- End of check Management -Account table -->		
				
						<!-- Start of check management Granular Previlages table -->
						{{#each assets}}
							{{#selectAsset assetId "14"}}
								{{#if granularPermission.checks}}
									<div class="ft-margin-t" id="brGranularDiv">
										<div style="max-width:1268px !important;overflow-x:auto; width: 100%;">
											<table class="view-grid" style="width:100%">
												<thead>
													<tr><td colspan="25" class="view-grid-section-header">Granular Privileges</td></tr>
													<tr class="view-grid-header">
														<td class="view-grid-header-text" style="width:20%;">Account</td>
														<td class="view-grid-header-text" style="width:20%;">Account Name</td>
														{{#each (splitOperation granularPermission.checksOperations) }}
															<td class="view-grid-header-text hidden" style="width:20%;" id="grCheckColHdr_{{@index}}">{{colName}}</td>
														{{/each}}
													</tr>												
												</thead>
												<tbody>							
													{{#each granularPermission.checks}}
															<tr class="view-grid-body">
																<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >{{getColoredExpression colorClass accountNo}}</td>														
																<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >{{getColoredExpression colorClass accountName}}</td>
																{{#each (splitMask mask) }}
																	{{#if isAssign}}
																		<td class="view-grid-header-text view-grid-body-content hidden"  id="chkGranView_{{../../assetId}}_{{@index}}_{{../accountId}}" style="width:20%;" >
																			<i class="fa fa-check"></i>
																		</td>														
																	{{else}}
																		<td class="view-grid-header-text view-grid-body-content hidden" id="chkGranView_{{../../assetId}}_{{@index}}_{{../accountId}}" style="width:20%;" ></td>
																	{{/if}} 
																{{/each}}														
															</tr>
													{{else}}
														<tr class="view-grid-body">
																<td class="view-grid-body-content" colSpan="3" style="padding-bottom: 1%;" >&nbsp</td>
														</tr>		
													{{/each}}											
												</tbody>
											</table>
										</div>
									</div>
								{{/if}}
							{{/selectAsset}}
						{{/each}}
						
						<!-- end of granular previlages table -->	
											
					</div>
				</div>
			</div>
			