			<div class="col-sm-6">
				{{>reportTemplate assetid="13"}}
			</div>
			<!-- End of Reports -->
		</div>
		<!-- Alerts and Reports Privileges ends here -->
	<div class="row">

		<!-- Start of Widgets -->
		<div class="col-sm-6">
			{{>widgetTemplate assetid="13"}}
		</div>
		<!-- End of Widgets -->
	</div>	
	
	
	<!--Granular Privileges - PP starts here -->	
		<div class="row">
			<div class="col-sm-12">
				{{>granPositivePayTemplate assetid="13"}}
