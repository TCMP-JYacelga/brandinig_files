				<a id="expandAll">{{getLabel "lbl.role.ppexpandAll" "Expand/"}}</a> <a id="collapseAll">{{getLabel "lbl.role.ppcollapseAll" "Collapse All"}}</a>
				</div>
			</div>
		</div>
	</div>
	{{#each assets}} 
		{{#selectAsset assetId "13"}}
	
		{{>prevTemplate assetid="13"}}

		<!--Account Section starts here -->
