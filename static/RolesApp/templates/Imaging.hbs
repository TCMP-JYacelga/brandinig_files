<div>
<div>
		<div class="row">
			<div class="col-sm-12 ft-padding-b">
				<div class="pull-right">
				<a id="expandAll">{{getLabel "lbl.role.expandAll" "Expand/"}}</a> <a id="collapseAll">{{getLabel "lbl.role.collapseAll" "Collapse All"}}</a>
				</div>
			</div>
		</div>
	</div>
	{{#each assets}} 
		{{#selectAsset assetId "16"}}
	
		{{>prevTemplate assetid="16"}}

		<!--Account Section starts here -->
		<div class="row">
			<div class="col-sm-12">
				{{>accountTemplate assetid="16"}}
			</div>
		</div>
		<!--Account Section end here -->
		

		<!--Alerts and Reports Privileges starts here -->
		<div class="row">
	
			<!-- Start of Alerts -->
			<div class="col-sm-6">
				{{>alertTemplate assetid="16"}}
			</div>
			<!-- End of Alerts -->
	
			<!-- Start of Reports -->
			<div class="col-sm-6">
				{{>reportTemplate assetid="16"}}
			</div>
			<!-- End of Reports -->
		</div>
		<!-- Alerts and Reports Privileges ends here -->
	
		{{/selectAsset}} 
	{{/each}}
</div>