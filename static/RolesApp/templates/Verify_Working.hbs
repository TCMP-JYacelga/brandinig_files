											{{/selectAsset}}
										{{/each}}
								</tbody>
							</table>							
						</div>
						<!-- End of BR -Account table -->			
						<!-- Start of BR Service - Privileges Table -->
						<div class="panel-body" id="brPrivileges">
							<table class="view-grid" style="width:100%">
								<thead>
									<tr><td colspan="25" class="view-grid-section-header">Privileges</td></tr>
									<tr class="view-grid-header">
										<th class="view-grid-header-text" >Type</th>
										<th class="view-grid-header-text" style="width:60%;">View</th>												
									</tr>		
								</thead>
								<tbody>							
										{{#each assets}}
											{{#selectAsset assetId "01"}}
												{{#each permissions}}
									<tr class="view-grid-body">
										<td class="view-grid-header-text view-grid-body-content" >{{screenName}}</td>
														{{#if view}}
