														{{/if}}														
									</tr>											
												{{/each}}											
											{{/selectAsset}}
										{{/each}}
								</tbody>
							</table>																				
						</div>
						<!-- End of BR Service - Privileges Table -->
						<!-- Start of BR Granular Previlages table -->
						<div class="panel-body" id="brGranularDiv">
							<div style="max-width:1268px !important;overflow-x:auto; width: 100%;">
								<table class="view-grid" style="width:100%">
									<thead>
										<tr><td colspan="25" class="view-grid-section-header">Granular Privileges</td></tr>
										<tr class="view-grid-header">
											<th class="view-grid-header-text" style="width:20%;">Account</th>
													{{#each assets}}
														{{#selectAsset assetId "01"}}
															{{#each granularPermission}}
																{{#if @first}}
																	{{#each (splitOperation operations) }}
											<th class="view-grid-header-text" style="width:20%;" >{{colName}}</th>
																	{{/each}}														
																{{/if}}												
															{{/each}}											
														{{/selectAsset}}
													{{/each}}
										</tr>												
									</thead>
									<tbody>							
											{{#each assets}}
												{{#selectAsset assetId "01"}}
													{{#each granularPermission}}
										<tr class="view-grid-body">
											<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >{{accountName}}</td>
															{{#each (splitMask mask) }}
