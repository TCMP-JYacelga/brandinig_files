<div class="ft-layout-primary">
	<div class="ft-layout-inner">
		<!--START:Page content ################-->
		<div id="PageTitle">
			<h1 class="ft-title">
				<span>View Roles</span>
			</h1>
			<div class="ft-accordion-header-sidebar pull-right title-content">
				<div class="ft-pdf-btn ft-icons-pdf ft-icons" title="Report">
				</div>
			</div>
		</div>
		<!-- start of verifyForm -->
		<div class="largepadding_tb" id="verifyForm">
			<!-- Start of Role Details Panel -->
			<div>
				<div class="panel panel-default">
					<div class="panel-heading">
						<div class="row">
							<div class="col-sm-12">
								<h2 class="panel-title">
									<span class="ft-padding-l ">Role Details</span>
								</h2>
							</div>
						</div>
					</div>
					<div class="panel-body" id="categoryDetails">
						<div class="ft-margin-t">
							<table class="view-grid" style="width:100%">
								<tbody>
									<tr class="view-grid-header"><th class="view-grid-header-text">Function</td><td class="view-grid-header-text" style="width:20%;">Details</th></tr>
									<tr class="view-grid-body"><td class="view-grid-body-content">Code</td><td class="view-grid-body-content" style="width:80%;">{{role}}</td></tr>
									<tr class="view-grid-body"><td class="view-grid-body-content">Description</td><td class="view-grid-body-content" style="width:80%;">{{role}}</td></tr>
									<tr class="view-grid-body"><td class="view-grid-body-content">Subsidiary Access</td><td class="view-grid-body-content" style="width:80%;">{{#each subsidiaries}}{{subsidiaryName}} {{#unless @last}},{{/unless}} 
										{{else}}
										None Assigned
									{{/each}}</td></tr>
									<tr class="view-grid-body"><td class="view-grid-body-content">Services</td><td class="view-grid-body-content" style="width:80%;">{{#each allowedServices}}{{serviceName}} {{#unless @last}},{{/unless}} 
										{{else}}
										None Assigned
									{{/each}}</td></tr>
									<tr class="view-grid-body"><td class="view-grid-body-content">Granular Privileges</td><td class="view-grid-body-content" style="width:80%;">{{#each allowedGranularPrivileges}}{{serviceName}} {{#unless @last}},{{/unless}}
											{{else}}
											None Assigned
											{{/each}}</td></tr>
								</tbody>
							</table>
						</div>
					</div>
				</div>
			</div>
			<!-- End of Role Details Panel -->
			<!-- Start of Admin Service Panel -->
			<div>
				<div class="panel panel-default">						
					<div class="panel-heading">
						<div class="row">
							<div class="col-sm-12">
								<h2 class="panel-title">									
									<span id='adminDetailsCaret'
													onClick="toggleSection('adminDetailsCaret', 'adminAssetBody')"
													class="fa fa-caret-up pull-left cursor_pointer"></span>Admin Services <span
													class="ft-padding-l "></span>										
								</h2>
							</div>
						</div>
					</div>
					<div id="adminAssetBody">
						<div class="panel-body" id="adminFunctionalDiv">
							<table class="view-grid" style="width:100%">
								<tbody>
									<tr class="view-grid-header"><th class="view-grid-header-text">Function</td><td class="view-grid-header-text" style="width:20%;">Details</th></tr>
									<tr class="view-grid-body"><td class="view-grid-body-content">Messages</td><td class="view-grid-body-content" style="width:80%;">
										{{#each assets}}{{#selectAsset assetId "03"}}{{#each messages}}
											{{messageType}}{{#unless @last}},{{/unless}}{{else}}None Assigned{{/each}}
														{{/selectAsset}}
										{{/each}}
										</td></tr>
									<tr class="view-grid-body"><td class="view-grid-body-content">Reports</td><td class="view-grid-body-content" style="width:80%;">{{#each assets}}{{#selectAsset assetId "03"}}{{#each reports}}
											{{reportType}}{{#unless @last}},{{/unless}}{{else}}None Assigned{{/each}}
														{{/selectAsset}}
										{{/each}}</td></tr>
									<tr class="view-grid-body"><td class="view-grid-body-content">Widgets</td><td class="view-grid-body-content" style="width:80%;">{{#each assets}}{{#selectAsset assetId "03"}}{{#each widgets}}
											{{widgetType}}{{#unless @last}},{{/unless}}{{else}}None Assigned{{/each}}
														{{/selectAsset}}
										{{/each}}</td></tr>								
								</tbody>
							</table>
						</div>	
						<!-- Start of Admin Service - Interface Privileges Table -->
						<div class="panel-body" id="adminInterfacePrivileges">
							<table class="view-grid" style="width:100%">
								<thead>
									<tr><td colspan="25" class="view-grid-section-header">Interface Privileges</td></tr>
									<tr class="view-grid-header">
										<th class="view-grid-header-text" style="width:20%;" >Interface Name</th>
										<th class="view-grid-header-text" style="width:20%;" >Edit</th>
										<th class="view-grid-header-text" style="width:20%;">Execute</th>
									</tr>		
								</thead>
								<tbody>							
										{{#each assets}}
											{{#selectAsset assetId "03"}}
												{{#each interfaces}}
									<tr class="view-grid-body">
										<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >{{interfaceType}}</td>
														{{#if edit}}
										<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >
											<i class="fa fa-check"></i>
										</td>
														{{else}}
										<td class="view-grid-header-text view-grid-body-content" style="width:20%;" ></td>
														{{/if}} 										
														{{#if execute}}
										<td class="view-grid-header-text view-grid-body-content" style="width:20%;" >
											<i class="fa fa-check"></i>
										</td>
														{{else}}
										<td class="view-grid-header-text view-grid-body-content" style="width:20%;" ></td>
														{{/if}}																	
									</tr>											
												{{/each}}											
											{{/selectAsset}}
										{{/each}}
								</tbody>
							</table>																				
						</div>
						<!-- End of Admin Service - Interface Privileges Table -->	
						<!-- Start of Admin Service - Privileges Table -->
